@font-face {
	font-family: 'iconos';
	src:url('fonts/icomoon.eot?-dltlyk');
	src:url('fonts/icomoon.eot?#iefix-dltlyk') format('embedded-opentype'),
		url('fonts/icomoon.woff?-dltlyk') format('woff'),
		url('fonts/icomoon.ttf?-dltlyk') format('truetype'),
		url('fonts/icomoon.svg?-dltlyk#icomoon') format('svg');
	font-weight: normal;
	font-style: normal;
}
coloriconos = #064370

